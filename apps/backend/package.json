{
    "name": "ironwing-backend",
    "version": "0.5.1",
    "description": "Server side of iron-wing-dispatching.com handling form submissions, leads database and integration with CMR..",
    "type": "module",
    "main": "app.js",
    "scripts": {
        "start": "node src/app.js",
        "start:dev": "dotenv -e .env.development -- nodemon src/app.js",
        "start:prod": "dotenv -e .env.production -- node src/app.js",
        "start:test:local": "dotenv -e .env.test -- node src/app.js",
        "test": "dotenv -e .env.test -- jest --passWithNoTests",
        "lint": "eslint src --ext .js",
        "lint:fix": "eslint src --ext .js --fix",
        "build": "babel src -d dist",
        "db:seed:test": "dotenv -e .env.test -- node __tests__/seed-test-db.js",
        "db:seed:test:user": "dotenv -e .env.test -- node __tests__/seed-test-user.js",
        "prisma:generate": "prisma generate",
        "db:migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
        "db:migrate:prod": "dotenv -e .env.production -- prisma migrate deploy"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@casl/ability": "^6.7.3",
        "@prisma/client": "^6.4.1",
        "bcryptjs": "^3.0.2",
        "body-parser": "^2.2.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "date-fns": "4.1.0",
        "dotenv": "^17.2.3",
        "dotenv-flow": "^4.1.0",
        "express": "^5.1.0",
        "express-rate-limit": "^8.1.0",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
        "nodemailer": "^7.0.7",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "redis": "^5.8.3"
    },
    "devDependencies": {
        "dotenv-cli": "^10.0.0",
        "nodemon": "^3.0.1",
        "prisma": "^6.4.1",
        "supertest": "^7.1.4"
    }
}
